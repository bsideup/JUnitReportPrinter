<?xml version="1.0" encoding="UTF-8"?><project name="Sweets" basedir="." xmlns:ivy="antlib:org.apache.ivy.ant">	<import file="build.ivy.xml" />	<target name="publish" depends="resolve, compile" description="Publish">		<ivy:publish resolver="local" forcedeliver="true">			<artifacts pattern="${basedir}/out/[artifact].[ext]" />		</ivy:publish>	</target>	<target name="compile" depends="resolve">		<mkdir dir="out" />		<copy todir="out" description="Copying output files">			<fileset dir="src">				<include name="JUnitReportPrinter.xml" />			</fileset>		</copy>	</target></project>